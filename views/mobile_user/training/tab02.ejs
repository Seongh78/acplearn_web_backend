<table class="ui unstackable table celled" style="padding:0; ">
    <colgroup>
        <col width="22%">
    </colgroup>
    <% for(var ii=1; ii<=3; ii++){ %>
    <tr>
        <td>
            <h4><%=ii%>조 </h4>
            <div class="ui list" style="font-size:0.85em;">
              <div class="item">팀: <b>35점</b> </div>
              <div class="item">개별: <b>25점</b> </div>
              <div class="item">합계: <b>60점</b> </div>
            </div>
        </td>
        <td>
            <div class="ui bulleted list" style="font-size:0.85em;">
              <div class="item">(주)비전플랜 / 박길동 / 개발팀 / 팀장 / 5점</div>
              <div class="item">(주)아카스타 / 정길동 / 경영팀 / 사원 / 15점</div>
              <div class="item">(주)블루킹스 / 홍길동 / 홍보팀 / 팀장 / 0점</div>
            </div>
        </td>
    </tr>
    <% } %>
    <tr>
        <td colspan="2">
            <h4>팀 미지정 인원</h4>
            <div class="ui relaxed divided list">
            <% for(var iii=1; iii<=5; iii++){%>
              <div class="item">
                <i class="large user circle icon"></i>
                <div class="content">
                  <a class="header">박길동</a>
                  <div class="description">(주)액플런 / 경영팀 / 팀장 / 15점</div>
                </div>
              </div>
              <% } %>

            </div>
        </td>
    </tr>
</table>



<div class="ui modal fullscreen">
  <div class="header">팀추가</div>
  <div class="content">
    <p></p>
    <p></p>
    <p></p>
  </div>
</div>



<script type="text/javascript">
$(document).on('click', '.addTeam', () => {
    $('.fullscreen.modal').modal('show');
})

</script>
